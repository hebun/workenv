<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">
	<application>
		<locale-config>
			<default-locale>tr</default-locale>
			<supported-locale>tr</supported-locale>
			<supported-locale>en</supported-locale>
		</locale-config>
		<resource-bundle>
			<base-name>util.Text</base-name>
			<var>msg</var>
		</resource-bundle>
	</application>
	<converter>
		<description></description>
		<converter-id>MemberConverter</converter-id>
		<converter-class>MemberConverter</converter-class>
	</converter>
	<converter>
		<description></description>
		<converter-id>CartConverter</converter-id>
		<converter-class>CartConverter</converter-class>
	</converter>
	<managed-bean>
		<managed-bean-name>app</managed-bean-name>
		<managed-bean-class>App</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>login</managed-bean-name>
		<managed-bean-class>Login</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>

		<managed-bean-name>members</managed-bean-name>
		<managed-bean-class>Members</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>carts</managed-bean-name>
		<managed-bean-class>Carts</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>process</managed-bean-name>
		<managed-bean-class>Process</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
</faces-config>
