package work;

import java.util.Iterator;

public class xxx implements Iterator<String> {
	String[] dizi = { "blbla", "xxxx", "yyy" };
	int k = 0;

	public xxx() {

	}

	@Override
	public boolean hasNext() {

		return k < 3;
	}

	@Override
	public String next() {

		return dizi[k++];
	}

	@Override
	public void remove() {
		throw new Exception("cant do that");

	}

}
